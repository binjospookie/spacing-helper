{
  "name": "spacing-helper",
  "version": "1.1.8",
  "description": "Standalone helper for creating consistent spacing between the elements of your UI",
  "main": "build/index.es.js",
  "types": "build/index.d.ts",
  "license": "MIT",
  "author": "Viktor Pasynok",
  "email": "binjospookie@gmail.com",
  "keywords": [
    "cssinjs",
    "emotion",
    "styled-components",
    "ui",
    "helper",
    "spacing"
  ],
  "private": false,
  "sideEffects": false,
  "files": [
    "build/index.es.js",
    "build/index.d.ts"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:binjospookie/spacing-helper.git"
  },
  "bugs": {
    "url": "https://github.com/binjospookie/spacing-helper/issues"
  },
  "homepage": "https://github.com/binjospookie/spacing-helper#readme",
  "scripts": {
    "build:js": "rollup -c ./rollup.config.js",
    "build:types": "tsc && scp ./dist/index.d.ts ./build",
    "build": "rm -rf ./build && npm run build:js && npm run build:types",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "prepublishOnly": "npm run build",
    "prettier": "prettier --check \"src/**/*.ts\"",
    "size": "npm run build && size-limit",
    "test": "ava"
  },
  "devDependencies": {
    "@arkweid/lefthook": "0.7.0",
    "@rollup/plugin-typescript": "2.1.0",
    "@size-limit/preset-small-lib": "3.0.0",
    "ava": "2.4.0",
    "esm": "3.2.25",
    "prettier": "1.19.1",
    "rollup": "1.29.0",
    "rollup-plugin-terser": "5.2.0",
    "ts-node": "8.6.2",
    "tslint": "5.20.1",
    "typescript": "3.7.5"
  },
  "ava": {
    "compileEnhancements": false,
    "concurrency": 5,
    "extensions": [
      "ts"
    ],
    "failFast": true,
    "require": [
      "esm",
      "ts-node/register"
    ]
  }
}
