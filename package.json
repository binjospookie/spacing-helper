{
  "name": "spacing-helper",
  "version": "1.0.5",
  "description": "Standalone helper for creating consistent spacing between the elements of your UI",
  "main": "build/index.es.js",
  "types": "build/index.d.ts",
  "license": "MIT",
  "author": "v.pasynok",
  "email": "binjospookie@gmail.com",
  "private": false,
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:binjospookie/spacing-helper.git"
  },
  "bugs": {
    "url": "https://github.com/binjospookie/spacing-helper/issues"
  },
  "homepage": "https://github.com/binjospookie/spacing-helper#readme",
  "scripts": {
    "build": "rm -rf ./build && npm run prepublish",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "prepublish": "npx rollup -c ./rollup.config.js",
    "prettier": "prettier --check \"src/**/*.ts\"",
    "size": "npm run build && size-limit",
    "test": "ava"
  },
  "devDependencies": {
    "@arkweid/lefthook": "0.6.7",
    "@size-limit/preset-small-lib": "2.2.2",
    "ava": "2.4.0",
    "esm": "3.2.25",
    "prettier": "1.19.1",
    "rollup-plugin-terser": "5.1.3",
    "rollup-plugin-typescript2": "0.25.3",
    "ts-node": "8.5.4",
    "tslint": "5.20.1",
    "typescript": "3.7.3"
  },
  "ava": {
    "compileEnhancements": false,
    "concurrency": 5,
    "extensions": [
      "ts"
    ],
    "failFast": true,
    "require": [
      "esm",
      "ts-node/register"
    ]
  }
}
