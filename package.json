{
  "name": "spacing-helper",
  "version": "4.1.1",
  "description": "Standalone helper for creating consistent spacing between the elements of your UI",
  "source": "src/index.js",
  "main": "build/index.js",
  "module": "build/index.module.js",
  "types": "build/index.d.ts",
  "license": "MIT",
  "author": "Viktor Pasynok",
  "email": "binjospookie@gmail.com",
  "keywords": [
    "cssinjs",
    "emotion",
    "styled-components",
    "ui",
    "helper",
    "spacing"
  ],
  "private": false,
  "sideEffects": false,
  "files": [
    "build"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:binjospookie/spacing-helper.git"
  },
  "bugs": {
    "url": "https://github.com/binjospookie/spacing-helper/issues"
  },
  "homepage": "https://github.com/binjospookie/spacing-helper#readme",
  "scripts": {
    "build:js": "microbundle --compress --no-sourcemap --no-raw",
    "copy:types": "cp src/index.d.ts ./build/index.d.ts",
    "build": "rm -rf ./build && yarn run build:js && yarn run copy:types",
    "lint": "eslint 'src/index.js'",
    "prepublishOnly": "yarn run build",
    "prettier": "prettier --check 'src/index.js'",
    "size": "yarn run build && size-limit",
    "test": "jest --coverage --no-cache"
  },
  "devDependencies": {
    "@arkweid/lefthook": "0.7.2",
    "@size-limit/preset-small-lib": "4.9.0",
    "benchmark": "2.1.4",
    "eslint": "7.14.0",
    "jest": "26.6.3",
    "microbundle": "0.12.4",
    "prettier": "2.2.1",
    "size-limit": "4.9.0"
  }
}
