{
  "name": "spacing-helper",
  "version": "4.0.0",
  "description": "Standalone helper for creating consistent spacing between the elements of your UI",
  "source": "src/index.ts",
  "main": "build/index.js",
  "module": "build/index.module.js",
  "types": "build/index.d.ts",
  "license": "MIT",
  "author": "Viktor Pasynok",
  "email": "binjospookie@gmail.com",
  "keywords": [
    "cssinjs",
    "emotion",
    "styled-components",
    "ui",
    "helper",
    "spacing"
  ],
  "private": false,
  "sideEffects": false,
  "files": [
    "build"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:binjospookie/spacing-helper.git"
  },
  "bugs": {
    "url": "https://github.com/binjospookie/spacing-helper/issues"
  },
  "homepage": "https://github.com/binjospookie/spacing-helper#readme",
  "scripts": {
    "build:js": "microbundle --compress --no-sourcemap --no-raw",
    "build:types": "cp src/types.ts ./build/index.d.ts",
    "build": "rm -rf ./build && npm run build:js && npm run build:types",
    "lint": "eslint 'src/index.ts'",
    "prepublishOnly": "npm run build",
    "prettier": "prettier --check 'src/index.ts'",
    "size": "npm run build && size-limit",
    "test": "jest --coverage --no-cache"
  },
  "devDependencies": {
    "@arkweid/lefthook": "0.7.2",
    "@size-limit/preset-small-lib": "4.5.6",
    "@types/jest": "26.0.10",
    "@typescript-eslint/eslint-plugin": "3.9.1",
    "@typescript-eslint/parser": "3.9.0",
    "benchmark": "2.1.4",
    "eslint": "7.7.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-prefer-arrow": "1.2.2",
    "jest": "26.4.0",
    "microbundle": "0.12.3",
    "prettier": "2.0.5",
    "size-limit": "4.5.6",
    "ts-jest": "26.2.0",
    "tslint": "6.1.3",
    "typescript": "3.9.7"
  }
}
